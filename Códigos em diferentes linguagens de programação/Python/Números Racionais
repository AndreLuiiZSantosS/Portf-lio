par = impar = negativos = positivos = 0 
for i in range (5) :
  n = int (input ())
  if n > 0:
    positivos+=1 
  if n < 0:
    negativos+=1
  if (n % 2 == 0):
    par+=1
  else: 
    impar+=1 
print (par,"valor(es) par(es)")
print (impar,"valor(es) impar(es)")
print (positivos,"valor(es) positivo(s)")
print(negativos,"valor(es) negativo(s)")
